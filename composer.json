{
    "name": "exertion/magma",
    "description": "The Magma WordPress Plugin Framework.",
    "type": "project",
    "license": "MIT",
    "support": {
        "source": "https://github.com/exertion/magma",
        "docs": "https://magma.exertion.co"
    },
    "require": {
        "php": "^7.2",
        "exertion/magma-framework": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
